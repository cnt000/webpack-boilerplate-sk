{
  "name": "webpack-boilerplate-sk",
  "version": "1.0.0",
  "description": "Webpack boilerplate for Monrif.net",
  "private": true,
  "scripts": {
    "test": "jest",
    "build": "webpack --config ./config/webpack.prod.cjs",
    "watch": "webpack --config ./config/webpack.dev.cjs --watch",
    "format": "prettier-eslint --write \"$(pwd)/**/*.js\"",
    "lint": "eslint . --config .eslintrc.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/192.168.1.1/sturie.git"
  },
  "keywords": [
    "webpack",
    "js",
    "ts",
    "jest",
    "eslint",
    "prettier",
    "babel"
  ],
  "author": "Edoardo Gargano",
  "license": "MIT",
  "bugs": {
    "url": "git@github.com:cnt000/webpack-boilerplate-sk.git"
  },
  "homepage": "git@github.com:cnt000/webpack-boilerplate-sk.git#readme",
  "dependencies": {},
  "babel": {
    "presets": [
      "@babel/preset-modules",
      "@babel/preset-react"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-modules": "^0.1.4",
    "@babel/preset-react": "^7.12.5",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^4.3.0",
    "mini-svg-data-uri": "^1.2.3",
    "prettier": "^2.1.2",
    "prettier-eslint-cli": "^5.0.0",
    "pretty-quick": "^3.1.0",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.3.0"
  },
  "sideEffects": false,
  "volta": {
    "node": "14.13.1"
  }
}
